{
  "name": "cube-client",
  "displayName": "cube-client",
  "description": "",
  "version": "0.0.1",
  "publisher": "hrntknr",
  "repository": {
    "type": "git",
    "url": "https://github.com/cynack/vscode-cube-client.git"
  },
  "engines": {
    "vscode": "^1.20.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.connectServer"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.connectServer",
        "title": "Connect to cube server"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/node": "^7.0.43",
    "@types/uuid": "^3.4.3",
    "@types/ws": "^4.0.1",
    "chai": "^4.1.2",
    "tslint": "^5.9.1",
    "tslint-config-standard": "^7.0.0",
    "typescript": "^2.6.1",
    "vscode": "^1.1.6"
  },
  "dependencies": {
    "javascript-stringify": "^1.6.0",
    "uuid": "^3.2.1",
    "ws": "^4.1.0"
  }
}
